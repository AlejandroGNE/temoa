strict digraph model {
	label = "Activity split of process TXG, 1980 in year 2000" ;

	compound    = "True" ;
	concentrate = "True";
	rankdir     = "LR" ;
	splines     = "False" ;

	node [ style="filled" ] ;
	edge [ arrowhead="vee" ] ;

	subgraph cluster_slices {
		label = "1980 Capacity: 6.04" ;

		color = "lightgrey" ;
		rank  = "same" ;
		style = "filled" ;

		node [ color="white", shape="box" ] ;

		"inter, day" ;
		"inter, night" ;
		"summer, day" ;
		"summer, night" ;
		"winter, day" ;
		"winter, night" ;
	}

	subgraph energy_carriers {
		node [
		  color     = "lightsteelblue",
		  fontcolor = "black",
		  shape     = "circle"
		] ;

		"GSL" [ href="../commodities/rc_GSL_2000.svg" ] ;
		"TX"  [ href="../commodities/rc_TX_2000.svg" ] ;
	}

	subgraph inputs {
		edge [ color="firebrick" ] ;

		"GSL" -> "inter, day"    [ label="1.08" ] ;
		"GSL" -> "inter, night"  [ label="0.54" ] ;
		"GSL" -> "summer, day"   [ label="1.08" ] ;
		"GSL" -> "summer, night" [ label="0.54" ] ;
		"GSL" -> "winter, day"   [ label="2.16" ] ;
		"GSL" -> "winter, night" [ label="1.08" ] ;
	}

	subgraph outputs {
		edge [ color="forestgreen" ] ;

		"inter, day"    -> "TX" [ label="0.25" ] ;
		"inter, night"  -> "TX" [ label="0.12" ] ;
		"summer, day"   -> "TX" [ label="0.25" ] ;
		"summer, night" -> "TX" [ label="0.12" ] ;
		"winter, day"   -> "TX" [ label="0.50" ] ;
		"winter, night" -> "TX" [ label="0.25" ] ;
	}
}
