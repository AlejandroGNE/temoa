strict digraph result_commodity_FEQ {
	label       = "FEQ - 2000" ;

	compound    = "True" ;
	concentrate = "True" ;
	rankdir     = "LR" ;
	splines     = "True" ;

	node [ shape="box", style="filled" ] ;
	edge [
	  arrowhead  = "vee",
	  fontsize   = "8",
	  label      = "   ",
	  labelfloat = "False",
	  labelfontcolor = "lightgreen"
	  len        = "2",
	  weight     = "0.5",
	] ;

	"FEQ" [ color="lightsteelblue", href="../results/results2000.svg", shape="circle" ] ;

	subgraph used_techs {
		node [ color="darkseagreen" ] ;

		"E21"    [ href="../results/results_E21_2000.svg" ] ;
		"IMPFEQ" [ href="../results/results_IMPFEQ_2000.svg" ] ;
	}

	subgraph used_techs {
		node [ color="powderblue" ] ;

		// no nodes in this section
	}

	subgraph in_use_flows {
		edge [ color="forestgreen" ] ;

		"FEQ"    -> "E21" ;
		"IMPFEQ" -> "FEQ" ;
	}

	subgraph unused_flows {
		edge [ color="powderblue" ] ;

		// no edges in this section
	}
}
