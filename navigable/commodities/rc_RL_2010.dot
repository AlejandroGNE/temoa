strict digraph result_commodity_RL {
	label       = "RL - 2010" ;

	compound    = "True" ;
	concentrate = "True" ;
	rankdir     = "LR" ;
	splines     = "True" ;

	node [ shape="box", style="filled" ] ;
	edge [
	  arrowhead  = "vee",
	  fontsize   = "8",
	  label      = "   ",
	  labelfloat = "False",
	  labelfontcolor = "lightgreen"
	  len        = "2",
	  weight     = "0.5",
	] ;

	"RL" [ color="lightsteelblue", href="../results/results2010.svg", shape="circle" ] ;

	subgraph used_techs {
		node [ color="darkseagreen" ] ;

		"RL1" [ href="../results/results_RL1_2010.svg" ] ;
	}

	subgraph used_techs {
		node [ color="powderblue" ] ;

		// no nodes in this section
	}

	subgraph in_use_flows {
		edge [ color="forestgreen" ] ;

		"RL1" -> "RL" ;
	}

	subgraph unused_flows {
		edge [ color="powderblue" ] ;

		// no edges in this section
	}
}
